{
  "name": "@jnsdls/rx-react-render",
  "version": "1.0.1",
  "description": "Turn observable props into a render function with values.",
  "main": "dist/rx-react-render.cjs.js",
  "module": "dist/rx-react-render.esm.js",
  "browser": "dist/rx-react-render.umd.js",
  "scripts": {
    "prebuild": "yarn prettier && yarn run test",
    "build": "NODE_ENV=production rollup -c",
    "dev": "NODE_ENV=development rollup -c -w",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "prettier": "prettier --write \"./src/**/*.@(js)\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jnsdls/rx-react-render.git"
  },
  "keywords": [
    "rx",
    "react",
    "render",
    "function",
    "observable"
  ],
  "author": "Jonas Daniels",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jnsdls/rx-react-render/issues"
  },
  "homepage": "https://github.com/jnsdls/rx-react-render#readme",
  "devDependencies": {
    "babel-core": "6.26.3",
    "babel-plugin-external-helpers": "6.22.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react-app": "3.1.2",
    "coveralls": "3.0.2",
    "enzyme": "3.7.0",
    "enzyme-adapter-react-16": "1.6.0",
    "enzyme-to-json": "3.3.4",
    "jest": "23.6.0",
    "prettier": "1.14.3",
    "react": "16.5.2",
    "react-dom": "16.5.2",
    "rollup": "0.66.4",
    "rollup-plugin-babel": "3.0.7",
    "rollup-plugin-commonjs": "9.1.8",
    "rollup-plugin-node-resolve": "3.4.0",
    "rxjs": "6.3.3"
  },
  "peerDependencies": {
    "react": "^16.4.2",
    "rxjs": "^6.2.2"
  },
  "dependencies": {
    "prop-types": "^15.6.2"
  },
  "files": [
    "dist"
  ],
  "jest": {
    "testURL": "http://localhost/",
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ]
  }
}
